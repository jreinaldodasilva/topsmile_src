{
  "name": "topsmile",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^2.1.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "framer-motion": "^10.16.5",
    "luxon": "^3.7.1",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "server": "cd ./backend && npm run dev",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:frontend": "react-scripts test --testPathPattern=src/tests --watchAll=false --passWithNoTests",
    "test:backend": "cd ./backend && npm test",
    "test:e2e": "cypress run",
    "test:watch": "react-scripts test --testPathPattern=src/tests",
    "test:coverage": "react-scripts test --testPathPattern=src/tests --coverage --watchAll=false",
    "test:coverage:frontend": "react-scripts test --testPathPattern=src/tests --coverage --watchAll=false",
    "test:coverage:backend": "cd ./backend && npm run test:coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.13",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "cypress": "^15.1.0",
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "mongodb-memory-server": "^10.2.0",
    "msw": "^2.11.2",
    "node-fetch": "^3.3.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "whatwg-fetch": "^3.6.20"
  }
}
